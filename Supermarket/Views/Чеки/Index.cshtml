@model IEnumerable<Supermarket.Models.Товары_в_чеке.ИнфоЧек>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Номер_карты)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Время_открытия_чека)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Время_закрытия_чека)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Фамилия)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Имя)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.НомерКассы)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Наличные)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Номер_карты)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Время_открытия_чека)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Время_закрытия_чека)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Фамилия)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.Имя)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.НомерКассы)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Наличные)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID_чека }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID_чека }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID_чека })
        </td>
    </tr>
}

</table>
